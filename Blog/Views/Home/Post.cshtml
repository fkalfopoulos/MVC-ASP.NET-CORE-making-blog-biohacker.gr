@model Blog.Models.Story

@{
    ViewData["Title"] = "Post";
}
<link href="~/Trumb/Trumbowyg-master/dist/ui/trumbowyg.min.css" />
<div>
    <form asp-controller="Home" asp-action="Edit" method="post" enctype="multipart/form-data">
        <div>
            <label> Title</label>
            <input asp-for="Title" />
        </div>
        <br>
        <div>
            <label>Body</label>
            <input asp-for="Body" />
        </div>
        <div>
            <label>Image</label>
            <input asp-for="Photo" />
        </div>
        <input type="submit" value="submit" />
    </form>
</div>

@section Scripts{
   
    
    <script src="~/Trumb/Trumbowyg-master/dist/trumbowyg.min.js"></script>
    <script src="//ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>

    <script>
        $('#trumbowyg-demo').trumbowyg();
    </script>
}
